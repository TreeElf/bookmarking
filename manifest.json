{
    "manifest_version": 2,
    "name": "Better Bookmarking",
    "version": "0.0.5.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "background",
        "identity",
        "notifications",
        "storage",
        "alarms",
		"notifications",
		"downloads",
		"downloads.open",
		"tabs",
		"bookmarks",
        "https://www.googleapis.com/*",
        "https://*.googleusercontent.com/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "icon.png",
            "38": "icon.png"
        },
        "default_title": "Click to Sign in with Google"
    },
    "oauth2": {
        "client_id": "404713238625-7579b0p3k6lo4n1e98br99ger705apif.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/drive"
        ]
    },
    "options_page": "options.html",
    "key": ""
}
